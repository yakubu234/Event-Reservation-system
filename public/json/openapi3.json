        '422':
          description: Unprocessable Content
          headers:
            Host:
              schema:
                type: number
                example: 127.0.0.1:8000
            Date:
              schema:
                type: string
                example: Fri, 05 Aug 2022 11:09:57 GMT
            Connection:
              schema:
                type: string
                example: close
            X-Powered-By:
              schema:
                type: number
                example: PHP/8.1.8
            Cache-Control:
              schema:
