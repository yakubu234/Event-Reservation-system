                example: '59'
            Access-Control-Allow-Origin:
              schema:
                type: string
                example: '*'
          content:
            application/json:
              schema:
                type: object
              examples:
                example-0:
                  summary: Create an event error
                  value:
                    message: The event name has already been taken.
                    errors:
                      event_name:
                        - The event name has already been taken.
                example-1:
